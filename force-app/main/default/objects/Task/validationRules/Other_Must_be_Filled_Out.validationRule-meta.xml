<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Other_Must_be_Filled_Out</fullName>
    <active>false</active>
    <description>If Other is selected</description>
    <errorConditionFormula>AND(NOT( OR( IsPickVal($User.Region__c,&quot;EU&quot;), IsPickVal( $User.Business_Division__c,&quot;DN&quot;))),
ISPICKVAL(00N80000004Xhst ,&quot;Other&quot;), 
00N80000004Xhsy =&quot;&quot;
)</errorConditionFormula>
    <errorMessage>If &quot;Other&quot; is selected in the field &quot;Who is this for?&quot;, then a value must be entered in the &quot;Other&quot; field.</errorMessage>
</ValidationRule>
