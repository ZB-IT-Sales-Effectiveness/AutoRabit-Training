<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Created_Date__c</fullName>
    <description>This field contains the creation date.</description>
    <externalId>false</externalId>
    <formula>TEXT(YEAR(DATEVALUE(CreatedDate))) 
+ 
If(MONTH(DATEVALUE(CreatedDate))&lt;=9,&apos;0&apos;+TEXT(MONTH(DATEVALUE(CreatedDate))),TEXT(MONTH(DATEVALUE(CreatedDate)))) 
+ 
If(DAY(DATEVALUE(CreatedDate))&lt;=9,&apos;0&apos;+TEXT(DAY(DATEVALUE(CreatedDate))),TEXT(DAY(DATEVALUE(CreatedDate))))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Created Date</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
