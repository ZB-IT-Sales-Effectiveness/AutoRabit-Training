#footer-image {background:url("http://www.zimmer.com/images/common/bg-bottom-nav-left.png") no-repeat scroll center top transparent;display:inline;float:right;height:35px;width:45px;z-index:1;}
#footer-navigation ul {list-style:none outside none; margin:0; padding:0;}
#footer-navigation li {background:url("http://www.zimmer.com/images/common/bg-line-1.gif") no-repeat scroll left center transparent;float:left;padding:0 9px;position:relative;z-index:1;margin-left:0;}
#footer-navigation li#footer-first-nav {background:none repeat scroll 0 0 transparent;display:inline;}
#footer-navigation li.pipe {background:url("") no-repeat scroll left center transparent;float:left;padding:0 9px;position:relative;z-index:1;}
#footer-navigation a {color:#009DDC;float:left;font-size:12px;text-decoration:none;border:0 none;font-weight:normal;margin:0;padding:0;}
#footer-navigation a:hover {color:#005D9E;}

.topPanel span form div span span div {margin:0 auto;width:300px;}
.topPanel span form div span span table {height:20px;width:290px;}
.topPanel span form div span span table td:nth-of-type(odd) {width:25px!important;padding:0 10px!important;}
.messageText {width:200px!important;margin:0!important;}
.title {width:700px;margin:0 auto;color:#006BB5;font-size:30px;font-family:Arial,Helvetica,sans-serif;text-align:left;}
.topPanel span form div table{margin:0 auto;width:700px;background-color:#FFF;height:200px;border:1px solid #EF8F00;padding:20px 0 0 100px;text-align:left;}
.topPanel span form div table td:nth-of-type(odd){width:100px;padding-right:20px;}
.topPanel span form div table label {color:#333333;font-size:15px;font-family:Arial,Helvetica,sans-serif;font-weight:normal;}
.topPanel span form div table input[type="text"], .topPanel span form div table input[type="password"] {width:325px;border:1px solid #009CDB}
.topPanel span form div table input[type="submit"] {
    background:url("http://www.zimmer.com/images/common/btn-search-t22.png") no-repeat 0 0!important;
    border:medium none!important;
    color:#FFFFFF!important;
    font-weight:bold!important;
    font-size:14px!important;
    height:30px!important;
    width:100px!important;
    font-family:Arial,Helvetica,sans-serif!important;
    padding-bottom:5px!important;
    margin-left:50px;
}
.topPanel span form div table input[type="submit"]:hover {background-position:0 -35px!important;}
#loginPage\3ASurgeonPortalSiteLogin\3AloginComponent\3AloginForm\3AtheLinks{margin-left:35px;}
