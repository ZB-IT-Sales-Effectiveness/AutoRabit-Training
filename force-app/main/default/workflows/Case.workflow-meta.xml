    <rules>
        <fullName>Auto assignment of the Cases to PS-ROSA Imaging Queue</fullName>
        <actions>
            <name>PS_ROSA_Queue</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.RecordTypeId</field>
            <operation>equals</operation>
            <value>Personalized Solutions</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Subject</field>
            <operation>contains</operation>
            <value>ROSA,Robot,Robotic,Xray,X-Ray,Marker,Calibration</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Origin</field>
            <operation>equals</operation>
            <value>Personalized Solutions Call Center</value>
        </criteriaItems>
        <description>Auto assignment of the Cases to PS-ROSA Imaging Queue</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Clinical Graphics Case Assignment</fullName>
        <actions>
            <name>PS_Clinical_Graphic_Owner_update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 AND 2 AND (3 OR 4 OR 5 )</booleanFilter>
        <criteriaItems>
            <field>Case.RecordTypeId</field>
            <operation>equals</operation>
            <value>Personalized Solutions</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Origin</field>
            <operation>equals</operation>
            <value>Personalized Solutions Clinical Graphics,Personalized Solutions Call Center</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Subject</field>
            <operation>contains</operation>
            <value>Clinical</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Subject</field>
            <operation>contains</operation>
            <value>Graphics</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Subject</field>
            <operation>contains</operation>
            <value>Hips</value>
        </criteriaItems>
        <description>This rule will assign all cases created with clinical or graphic keywords and origin as Personalized Solutions Call Center through PS Email (personalizedsolutions@zimmerbiomet.com / us.support@clinicalgraphics.com)</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>EU-DE-Case Assignment Changed</fullName>
        <actions>
            <name>EU_DE_Case_Assignment</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(ISCHANGED(OwnerId), (RecordTypeId = &apos;01280000000Q0mD&apos;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
