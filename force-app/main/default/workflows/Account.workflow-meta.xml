    <rules>
        <fullName>Change Account Owner to Gustavo Quijano</fullName>
        <actions>
            <name>Change_Owner</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Change account owner for physician records where country - BM, VI  and PR to Gustavo Quijano.</description>
        <formula>AND( RecordType.Name == &apos;Physician&apos; ,  OR(PersonMailingCountry == &apos;BM&apos;,PersonMailingCountry == &apos;VI&apos;,PersonMailingCountry == &apos;PR&apos;, PersonMailingCountry == &apos;GU&apos;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Change Owner when Consumer Account is Created</fullName>
        <actions>
            <name>Change_owner_for_Consumer_Account</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>This workflow rule changes the owner when the Consumer is created or edited to Steve Barrett</description>
        <formula>( RecordTypeId = &quot;012800000003YfK&quot;)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Change Physician Account Owner to DCS SFDC for US Accounts</fullName>
        <actions>
            <name>Change_owner_for_surgeons_and_Hospitals</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>This workflow rule changes the owner when the Physician is created or edited to DCS SFDC</description>
