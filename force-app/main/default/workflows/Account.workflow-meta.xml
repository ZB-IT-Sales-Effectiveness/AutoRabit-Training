    <rules>
        <fullName>AP-ANZ-Change owner of HCP</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Account.OwnerId</field>
            <operation>equals</operation>
            <value>Events Site Guest User</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.PersonMailingCountry</field>
            <operation>equals</operation>
            <value>AU,NZ</value>
        </criteriaItems>
        <description>Changes the owner of accounts created through the GEM&apos;s process, with a mailing country of AU or NZ, to Ray Langenberg</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
